{
  "version": 2,
  "name": "hindu-yuva-website",
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/home",
      "dest": "/home.html"
    },
    {
      "src": "/about",
      "dest": "/aboutus.html"
    },
    {
      "src": "/events",
      "dest": "/events.html"
    },
    {
      "src": "/gallery",
      "dest": "/gallery.html"
    },
    {
      "src": "/resources",
      "dest": "/resources.html"
    },
    {
      "src": "/temple",
      "dest": "/temple.html"
    },
    {
      "src": "/academic",
      "dest": "/academic.html"
    },
    {
      "src": "/career",
      "dest": "/career.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
